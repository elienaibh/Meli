{
  "version": 2,
  "builds": [
    {
      "src": "meliautoprofit/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook/mercadolivre",
      "dest": "meliautoprofit/app/main.py"
    },
    {
      "src": "/auth/callback",
      "dest": "meliautoprofit/app/main.py"
    },
    {
      "src": "/",
      "dest": "meliautoprofit/app/main.py"
    }
  ]
} 